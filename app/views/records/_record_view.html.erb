<div>
  <table class="table table-condensed">
    <thead>
      <tr>
        <th>Name</th>
        <th>Start Date</th>
        <th>View</th>
        <th>Edit</th>
        <th>Delete</th>
      </tr>
   </thead>
   <% records.each do |record| %>
    <tr>
      <td id="name">
        <%=Client.find(record.client_id).first_name%>
      </td>
      <td id="name">
        <%=record.start_date.strftime("%b %d, %y")%>
      </td>
      <td><%= button_to('View', record_path(record), method: :get, :class => 'btn btn-default btn-sm')%>
      </td>
      <td><%= button_to('Edit', edit_record_path(record), method: :get, :class => 'btn btn-default btn-sm')%>
      </td>
      <td><%= button_to 'Delete', record_path(record), :class => 'btn btn-default btn-sm',
          method: :delete,
          data: { confirm: 'Are you sure?' } %></td>
      </td>
     <%end%>
  </table>
</div>